<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="github" value="https://nuget.pkg.github.com/rob893/index.json" />
  </packageSources>

  <packageSourceCredentials>
    <github>
      <add key="Username" value="%PACKAGE_GITHUB_USERNAME%" />
      <add key="ClearTextPassword" value="%PACKAGE_PAT%" />
    </github>
  </packageSourceCredentials>
</configuration>